<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/net"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_app">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appCompatTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/back_icon_b"
        android:drawablePadding="12dp"
        android:text="Result"
        android:textColor="#FFFFFFFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:src="@drawable/icon_end_dui"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appCompatTextView" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CLEAN FINISHED"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appCompatImageView" />

    <TextView
        android:id="@+id/tv_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Saved 400MB space for you"
        android:textColor="#FFFFFFFF"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="41dp"
        android:background="@drawable/bg_end_bottom"
        android:orientation="vertical"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_end">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_item_end_app"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="10dp"
            android:paddingVertical="12dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_item_end_icon"
                android:padding="5dp"
                android:src="@drawable/icon_app_end" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_policy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="App Manager"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mb_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Check"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp"
                app:backgroundTint="#0F54DC"
                app:cornerRadius="6dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="@drawable/bg_item_end_large"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="10dp"
            android:paddingVertical="12dp">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_item_end_icon"
                android:padding="5dp"
                android:src="@drawable/icon_large_files_end" />

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="Large files"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mb_large"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Check"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp"
                app:backgroundTint="#0F54DC"
                app:cornerRadius="6dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="@drawable/bg_item_end_dup"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="10dp"
            android:paddingVertical="12dp">

            <ImageView
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:background="@drawable/bg_item_end_icon"
                android:padding="5dp"
                android:src="@drawable/dup_icon" />

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="Duplicate Files"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mb_duplicate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Check"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp"
                app:backgroundTint="#0F54DC"
                app:cornerRadius="6dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="@drawable/bg_item_end_clean"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="10dp"
            android:paddingVertical="12dp">

            <ImageView
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:background="@drawable/bg_item_end_icon"
                android:padding="5dp"
                android:src="@drawable/sba" />

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="Clean"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/mb_clean"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Check"
                android:textColor="#FFFFFFFF"
                android:textSize="12sp"
                app:backgroundTint="#0F54DC"
                app:cornerRadius="6dp" />

        </LinearLayout>

    </LinearLayout>

    <include
        android:id="@+id/inLoad"
        layout="@layout/kuang_load" />
</androidx.constraintlayout.widget.ConstraintLayout>